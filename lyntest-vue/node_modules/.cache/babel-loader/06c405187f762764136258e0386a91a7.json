item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n      </el-form-item>\n\n      <el-form-item label=\"总件数\">\n        <el-input-number v-model=\"form.total_number\" :min=\"1\" :max=\"20\"></el-input-number>\n      </el-form-item>\n\n      <el-form-item label=\"批量开单\">\n        <el-input-number v-model=\"form.batch_orders\"  :min=\"1\" :max=\"20\"></el-input-number>\n      </el-form-item>\n    </el-form>\n\n    <div style=\"padding: 0px 12px\">\n      <el-button type=\"primary\" @click=\"getOrder\">立即创建</el-button>\n      <el-dialog title=\"订单号\" :visible.sync=\"order_list_dialog_Visible\">\n        <el-input type=\"textarea\" autosize  v-model=\"order_list\" v-loading=\"loading\"></el-input>\n        <span slot=\"footer\" class=\"dialog-footer\">\n              <el-button type=\"primary\" @click=\"order_list_dialog_Visible = false\" >确 定</el-button>\n            </span>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Orders from '@/model/dpf/orders'\nimport Ubt from '@/model/dpf/ubt'\n\n\nexport default {\n  data() {\n    return {\n      form: {\n        environment_id: 1,\n        channel: 'EWBRLB',\n        logistic_id: '',\n        customer_code: '',\n        delivery_province: '上海',\n        delivery_city: '上海市',\n        delivery_address: '徐泾镇明珠路1018号C区',\n        receiver_province: '上海',\n        receiver_city: '上海市',\n        receiver_address: '徐泾镇明珠路1018号C区',\n        transport_type: 'PACKAGE',\n        product_type: 1,\n        order_type: 1,\n        total_number: 1,\n        batch_orders: 1\n      },\n      environment_id_list: [{\n        label: '云1',\n        value: 1,\n      }, {\n        label: '云2',\n        value: 2,\n      }],\n      channel_list: [{\n        value: 'EWBRLB',\n        label: '货拉拉'\n      }, {\n        value: 'EWBSHRLBWLKJYXGS',\n        label: '飞蚂蚁'\n      }, {\n        value: 'ZHIFUBAO',\n        label: '支付宝'\n      }, {\n        value: 'HUAWEI',\n        la