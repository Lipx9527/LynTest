og_Visible = true\n      this.loading = true\n      this.order_list = await Orders.getNdopOrder(this.form)\n      this.loading = false\n\n      await Ubt.insertUbt('dop-order', this.form.batch_orders)\n    }\n  }\n}\n</script>\n\n<!--suppress CssUnknownTarget -->\n<style lang=\"scss\" scoped>\n  @import \"@/static/css/common.scss\";\n</style>\n"],"sourceRoot":"src/view/data-platform/order"}]}