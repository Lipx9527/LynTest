A,qBAAA,OAAA,GAAA,KAAA;AAlDA;AAAA,uBAoDA,GAAA,CAAA,SAAA,CAAA,YAAA,EAAA,KAAA,IAAA,CAAA,YAAA,CApDA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AArMA,CAAA","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"header\"><div class=\"title\">NVAS</div></div>\n\n    <el-form ref=\"form\" :model=\"form\" label-width=\"80px\" style=\"padding: 20px 0px\">\n      <el-form-item label=\"发件信息\">\n        <el-input v-model=\"form.delivery_name\" placeholder=\"姓名\" style=\"width: 120px\"></el-input>\n        <el-input v-model=\"form.delivery_mobile\" placeholder=\"手机号\" style=\"width: 130px\"></el-input>\n        <el-input v-model=\"form.delivery_tel\" placeholder=\"座机\" style=\"width: 130px\"></el-input>\n        <el-input v-model=\"form.delivery_storecode\" placeholder=\"门店编码\" style=\"width: 100px\"></el-input>\n      </el-form-item>\n\n      <el-form-item label=\"发件地址\">\n        <el-input v-model=\"form.delivery_country\" placeholder=\"国\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.delivery_province\" placeholder=\"省\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.delivery_city\" placeholder=\"市\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.delivery_area\" placeholder=\"区\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.delivery_town\" placeholder=\"街道\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.delivery_address\" placeholder=\"地址\" style=\"width: 200px\"></el-input>\n      </el-form-item>\n\n      <el-form-item label=\"收件信息\">\n        <el-input v-model=\"form.receiver_name\" placeholder=\"姓名\" style=\"width: 120px\"></el-input>\n        <el-input v-model=\"form.receiver_mobile\" placeholder=\"手机号\" style=\"width: 130px\"></el-input>\n        <el-input v-model=\"form.receiver_tel\" placeholder=\"座机\" style=\"width: 130px\"></el-input>\n        <el-input v-model=\"form.receiver_company\" placeholder=\"收件公司\" style=\"width: 100px\"></el-input>\n      </el-form-item>\n\n      <el-form-item label=\"收货地址\">\n        <el-input v-model=\"form.receiver_country\" placeholder=\"国\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.receiver_province\" placeholder=\"省\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.receiver_city\" placeholder=\"市\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.receiver_area\" placeholder=\"区\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.receiver_town\" placeholder=\"街道\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.receiver_address\" placeholder=\"地址\" style=\"width: 200px\"></el-input>\n      </el-form-item>\n\n      <el-form-item label=\"商品信息\">\n        <el-input v-model=\"form.weight\" placeholder=\"重量\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.volume\" placeholder=\"体积\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.cargo_name\" placeholder=\"货物名称\" style=\"width: 100px\"></el-input>\n      </el-form-item>\n\n      <el-form-item label=\"其他信息\">\n        <el-input v-model=\"form.platform_order\" placeholder=\"用户订单号\" style=\"width: 120px\"></el-input>\n        <el-input v-model=\"form.other_expenses\" placeholder=\"其他费用\" style=\"width: 100px\"></el-input>\n        <el-input v-model=\"form.insurance_amount\" placeholder=\"保价金额\" style=\"width: 100px\"></el-input>\n      </el-form-item>\n\n      <el-form-item label=\"产品类型\">\n        <el-radio-group v-model=\"form.product_type\" @change=\"handleProductType\" style=\"width: 150px\">\n          <el-radio-button v-for=\"item in product_type_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n        <el-select v-model=\"form.transport_label\" :value=\"form.transport_value\" style=\"padding: 0px 20px\">\n          <el-option v-for=\"item in transport_type_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.value\"></el-option>\n        </el-select>\n        <el-select v-model=\"form.ship_method_label\" :value=\"form.ship_method_value\" style=\"padding: 0px 20px\">\n          <el-option v-for=\"item in ship_method_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.value\"></el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-form-item label=\"付款方式\">\n        <el-radio-group v-model=\"form.payment_type\">\n          <el-radio-button v-for=\"item in payment_type_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n      </el-form-item>\n\n      <el-form-item label=\"代收货款\">\n        <el-radio-group v-model=\"form.bool_collecting\" @change=\"handleBoolCollecting\" style=\"width: 320px\">\n          <el-radio-button v-for=\"item in bool_collecting_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n        <el-input v-model=\"form.bool_collecting_value\" placeholder=\"请输入银行账号\" style=\"width: 250px;padding: 0px 20px\" :disabled=\"bool_collecting_disabled\"></el-input>\n      </el-form-item>\n\n      <el-form-item label=\"签收单\">\n        <el-radio-group v-model=\"form.signature_form\">\n          <el-radio-button v-for=\"item in signature_form_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n      </el-form-item>\n\n      <el-form-item label=\"订单类型\">\n        <el-radio-group v-model=\"form.order_type\">\n          <el-radio-button v-for=\"item in order_type_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n      </el-form-item>\n\n      <el-form-item label=\"子母件数\">\n        <el-input-number v-model=\"form.batch_goods_count\"  @change=\"handleBatchGoodsCount\" :min=\"1\" :max=\"9\" :disabled=\"form.batch_goods_orders_disabled\"></el-input-number>\n      </el-form-item>\n\n      <el-form-item label=\"批量开单\">\n        <el-input-number v-model=\"form.batch_orders\" :min=\"1\" :max=\"10\" :disabled=\"form.batch_orders_disabled\"></el-input-number>\n      </el-form-item>\n    </el-form>\n\n    <div style=\"padding: 0px 12px\">\n      <el-button type=\"primary\" @click=\"getOrderId\">立即创建</el-button>\n      <el-dialog title=\"订单号\" :visible.sync=\"order_list_dialog_visible\">\n        <el-input type=\"textarea\" autosize  v-model=\"order_list\" v-loading=\"loading\"></el-input>\n        <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\" @click=\"order_list_dialog_visible = false\" >确 定</el-button>\n      </span>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Orders from '@/model/dpf/orders'\nimport Ubt from '@/model/dpf/ubt'\n\nexport default {\n  data() {\n    return {\n      form: {\n        delivery_name: '发件人姓名',\n        delivery_mobile: '13333333333',\n        delivery_tel: '',\n        delivery_store_code: '',\n        delivery_country: '中国',\n        delivery_province: '上海',\n        delivery_city: '上海市',\n        delivery_area: '青浦区',\n        delivery_town: '徐泾镇',\n        delivery_address: '明珠路1018号C区',\n        receiver_name: '收件人姓名',\n        receiver_mobile: '15555555555',\n        receiver_tel: '',\n        receiver_company: '收件公司名称',\n        receiver_country: '中国',\n        receiver_province: '上海',\n        receiver_city: '上海市',\n        receiver_area: '青浦区',\n        receiver_town: '徐泾镇',\n        receiver_address: '明珠路1018号C区',\n        volume: '',\n        weight: '',\n        other_expenses: '', //  其他费用\n        insurance_amount: '',\n        cargo_name: '货物名称',\n        platform_order: '',\n        product_type: 1,\n        transport_label: '标准快递',\n        transport_value: 'PACKAGE',\n        ship_method_label: '送货上楼',\n        ship_method_value: 'PICKUPSTAIRS',\n        payment_type: 'PAY_ARIIVE',\n        signature_form: 'NO_RETURN_SIGNED',\n        order_type: 'EWAYBILL',\n        bool_collecting: 'NOTHING',\n        bool_collecting_value: '',\n        batch_goods_count: 1,\n        batch_orders: 1,\n      },\n      product_type_list: [{\n        value: 1,\n        label: '快递'\n      }, {\n        value: 2,\n        label: '零担'\n      }],\n      transport_type_list: [{\n        value: 'PACKAGE',\n        label: '标准快递'\n      }, {\n        value: 'RCP',\n        label: '3.60特惠件'\n      }, {\n        value: 'ZBRH',\n        label: '3.60特重件'\n      }, {\n        value: 'DEAP',\n        label: '特准快件'\n      }, {\n        value: 'JJDJ',\n        label: '经济大件'\n      }, {\n        value: 'WXJTH',\n        label: '微小件特惠'\n      }, {\n        value: 'ZBTH',\n        label: '重包特惠'\n      }, {\n        value: 'NZBRH',\n        label: '重包入户'\n      }],\n      exp_transport_type: [{\n        value: 'PACKAGE',\n        label: '标准快递'\n      }, {\n        value: 'RCP',\n        label: '3.60特惠件'\n      }, {\n        value: 'ZBRH',\n        label: '3.60特重件'\n      }, {\n        value: 'DEAP',\n        label: '特准快件'\n      }, {\n        value: 'JJDJ',\n        label: '经济大件'\n      }, {\n        value: 'WXJTH',\n        label: '微小件特惠'\n      }, {\n        value: 'ZBTH',\n        label: '重包特惠'\n      }, {\n        value: 'NZBRH',\n        label: '重包入户'\n      }],\n      ltl_transport_type: [{\n        value: 'JZKH/JZCY',\n        label: '精准卡航(电子面单)'\n      }, {\n        value: 'JZQY',\n        label: '精准汽运(电子面单)'\n      }, {\n        value: 'HK_JZKY',\n        label: '精准空运'\n      }, {\n        value: 'QC_JZKH',\n        label: '精准卡航'\n      }, {\n        value: 'QC_JZQYC',\n        label: '精准汽运'\n      }, {\n        value: 'PCP',\n        label: '精准包裹'\n      }],\n      ship_method_list: [{\n        value: 'PICKUPSTAIRS',\n        label: '送货上楼'\n      }],\n      exp_ship_method: [{\n        value: 'PICKUPSTAIRS',\n        label: '送货上楼'\n      }],\n      ltl_ship_method: [{\n        value: 'PICKSELF',\n        label: '自提'\n      }, {\n        value: 'PICKNOTUPSTAIRS',\n        label: '送货(不含上楼)'\n      }, {\n        value: 'PICKUPSTAIRS',\n        label: '送货上楼'\n      }, {\n        value: 'BIGUPSTAIRS',\n        label: '大件上楼'\n      }, {\n        value: 'SEND_UPSTAIRS_EQUIP',\n        label: '送货上楼(家装)'\n      }, {\n        value: 'SEND_AND_EQUIP',\n        label: '送货安装'\n      }, {\n        value: 'SEND_NO_UPSTAIRS',\n        label: '送货不上楼(家装)'\n      }],\n      payment_type_list: [{\n        value: 'PAY_ARIIVE',\n        label: '到付'\n      }, {\n        value: 'CASH',\n        label: '现金'\n      }, {\n        value: 'MONTH_PAY',\n        label: '月结'\n      }],\n      signature_form_list: [{\n        value: 'NO_RETURN_SIGNED',\n        label: '无须返单'\n      }, {\n        value: 'CUSTOMER_SIGNED_ORIGINAL',\n        label: '签收单原件返回'\n      }, {\n        value: 'CUSTOMER_SIGNED_FAX',\n        label: '签收单传真返回'\n      }],\n      order_type_list: [{\n        value: 'EWAYBILL',\n        label: '电子面单'\n      }, {\n        value: 'WAYBILL',\n        label: '散客电子面单'\n      }],\n      bool_collecting_list: [{\n        value: 'NOTHING',\n        label: '无'\n      }, {\n        value: 'NORMAL',\n        label: '三日退'\n      }, {\n        value: 'INTRADAY',\n        label: '即日退'\n      }, {\n        value: 'RA',\n        label: '审核退'\n      }],\n      bool_collecting_disabled: true,\n      batch_goods_orders_disabled: false,\n      batch_orders_disabled: false,\n      order_list_dialog_visible: false,\n      loading: false,\n      order_list: '', // 保存结果\n    }\n  },\n  methods: {\n    handleProductType(value) {\n      //  产品类型\n      if (value === 2) {\n        this.form.transport_type = this.form.ltl_transport_type\n        this.form.transport_label = '精准卡航(电子面单)'\n        this.form.transport_value = 'JZKH/JZCY'\n        this.form.ship_method = this.form.ltl_ship_method\n        this.form.ship_method_label = '自提'\n        this.form.ship_method_value = 'PICKSELF'\n        this.form.batch_goods_count = 1\n        this.batch_goods_orders_disabled = true\n        this.batch_orders_disabled = false\n      } else if (value === 1) {\n        this.form.transport_type = this.form.exp_transport_type\n        this.form.transport_label = '标准快递'\n        this.form.transport_value = 'PACKAGE'\n        this.form.ship_method = this.form.exp_ship_method\n        this.form.ship_method_label = '送货上楼'\n        this.form.ship_method_value = 'PICKUPSTAIRS'\n        this.batch_goods_orders_disabled = false\n      }\n      this.form.product_type = value\n    },\n    handleBoolCollecting(value) {\n      this.form.bool_collecting = value\n      if (value !== 'NOTHING') {\n        this.bool_collecting_disabled = false\n      } else if (value === 'NOTHING') {\n        this.bool_collecting_disabled = true\n        this.form.bool_collecting_value = ''\n      }\n    },\n    handleBatchGoodsCount(value) {\n      // 子母件数\n      this.form.batch_goods_count = value\n      if (value >= 2) {\n        this.form.goods_count = 2\n        this.batch_orders_disabled = true\n        this.form.batch_orders = 1\n      } else if (value === 1) {\n        this.form.goods_count = 1\n        this.batch_orders_disabled = false\n      }\n    },\n    async getOrderId() {\n      this.order_list = ''\n      this.order_list_dialog_visible = true\n      this.loading = true\n      this.order_list = await Orders.getNvasOrder(this.form)\n      this.loading = false\n\n      await Ubt.insertUbt('nvas-order', this.form.batch_orders)\n    }\n  }\n\n}\n</script>\n\n<!--suppress CssUnknownTarget -->\n<style lang=\"scss\" scoped>\n@import \"@/static/css/common.scss\";\n</style>\n"],"sourceRoot":"src/view/data-platform/order"}]}