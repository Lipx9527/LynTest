orm-item label=\"运单号\">\n        <el-tooltip class=\"item\" effect=\"dark\" content=\"支持批量传入单号，单号与单号间用英文逗号分隔\" placement=\"top-start\">\n          <el-input v-model=\"form.order_numbers\" placeholder=\"运单号\" style=\"width: 200px\"></el-input>\n        </el-tooltip>\n      </el-form-item>\n\n      <el-form-item label=\"签收类型\">\n        <el-select v-model=\"form.sign_status\">\n          <el-option v-for=\"item in sign_status_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.value\"></el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-form-item label=\"签收情况\">\n        <el-select v-model=\"form.sign_situation\">\n          <el-option v-for=\"item in sign_situation_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.value\"></el-option>\n        </el-select>\n      </el-form-item>\n    </el-form>\n    <div style=\"padding: 0px 80px\">\n      <el-button type=\"primary\" @click=\"execute\">执行</el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Terminal from '@/model/dpf/terminal'\nimport Ubt from '@/model/dpf/ubt'\n\nexport default {\n  data() {\n    return {\n      form: {\n        environment_id: 1,\n        department: '',\n        department_code: 'W0113080102',\n        orderNumbers: '',\n        sign_status: 'NORMAL_SIGN',\n        sign_situation: 'NORMAL_SIGN',\n      },\n      environment_id_list: [{\n        label: '云1',\n        value: 1,\n      }, {\n        label: '云2',\n        value: 2,\n      }],\n      department_list: this.GLOBAL.department,\n      sign_status_list: [{\n        label: '正常签收',\n        value: 'NORMAL_SIGN',\n      }, {\n        label: '异常签收',\n        value: 'UNNORMAL_SIGN',\n      }, {\n        label: '滞留标记',\n        value: 'RETENTION',\n      }],\n      sign_situation_list: [{\n        label: '正常签收',\n        value: 'NORMAL_SIGN',\n      }, {\n        label: '异常-破损',\n        value: 'UNNORMAL_BREAK',\n      }, {\n        label: '异常-潮湿',\n        value: 'UNNORMAL_DAMP',\n      }, {\n        label: '异常-内物短少',\n        value: 'UNNORMAL_GOODSHORT',\n      }, {\n        label: '异常-其他（非破损类）\\n',\n        value: 'UNNORMAL_ELSE',\n      }],\n    }\n  },\n  methods: {\n    departmentChange(key) {\n      this.department_list.forEach(d => {\n        if (d.value === key) {\n          this.form.department = d.label\n        }\n      })\n    },\n    async execute() {\n      const res = await Terminal.doExpSign(this.form)\n      if (res.success) {\n        this.$message.success(res.result)\n      } else {\n        this.$message.error(res.msg)\n      }\n\n      await Ubt.insertUbt('process-sign')\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  //noinspection CssUnknownTarget\n  @import \"@/static/css/common.scss\";\n</style>\n"]}]}