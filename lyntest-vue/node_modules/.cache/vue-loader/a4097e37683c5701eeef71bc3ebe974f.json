,\n}\n",{"version":3,"sources":["dls-storage-in.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqCA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"dls-storage-in.vue","sourceRoot":"src/view/data-platform/link","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"header\"><div class=\"title\">大力神入库</div></div>\n    <el-form ref=\"form\" :model=\"form\" label-width=\"80px\" style=\"padding: 20px 0px\">\n      <el-form-item label=\"环境\">\n        <el-radio-group v-model=\"form.environment_id\">\n          <el-radio-button v-for=\"item in environment_id_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n      </el-form-item>\n\n      <el-form-item label=\"部门\">\n        <el-select v-model=\"form.department\" style=\"width: 250px\">\n          <el-option v-for=\"item in department_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.value\"></el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-form-item label=\"主单号\">\n        <el-tooltip class=\"item\" effect=\"dark\" content=\"支持批量传入单号，单号与单号间用英文逗号分隔\" placement=\"top-start\">\n          <el-input v-model=\"form.order_number\" placeholder=\"主单号\" style=\"width: 200px\"></el-input>\n        </el-tooltip>\n      </el-form-item>\n\n      <el-form-item label=\"子母件数\">\n        <el-radio-group v-model=\"goods_count_switch\" style=\"width: 120px\"  @change=\"handleGoodsCount\">\n          <el-radio-button v-for=\"item in goods_count_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n        <el-input-number v-model=\"form.goods_count\"  @change=\"handleBatchGoodsCount\" :min=\"1\"></el-input-number>\n      </el-form-item>\n\n    </el-form>\n    <div style=\"padding: 0px 80px\">\n      <el-button type=\"primary\" @click=\"execute\">执行</el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Link from '@/model/dpf/link'\nimport Ubt from '@/model/dpf/ubt'\n\nexport default {\n  data() {\n    return {\n      form: {\n        environment_id: 1,\n        department: 'W0113080102',\n        order_number: '',\n        goods_count: 1,\n      },\n      environment_id_list: [{ label: '云1', value: 1 }],\n      department_list: this.GLOBAL.department,\n      goods_count_switch: 1, // 子母件，默认为单件\n      goods_count_list: [{\n        label: '否',\n        value: 1\n      }, {\n        label: '是',\n        value: 2\n      }],\n    }\n  },\n  methods: {\n    handleGoodsCount(value) {\n      this.form.goods_count = value\n    },\n    handleBatchGoodsCount(value) {\n      this.goods_count_switch = value >= 2 ? 2 : 1\n    },\n    async execute() {\n      const res = await Link.doDlsStorageIn(this.form)\n      if (res.success) {\n        this.$message.success('入库成功')\n      } else {\n        this.$message.error(res.msg)\n      }\n\n      await Ubt.insertUbt('storage-in')\n    },\n  },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n//noinspection CssUnknownTarget\n@import \"@/static/css/common.scss\";\n</style>\n"]}]}