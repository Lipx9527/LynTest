io-group>\n      </el-form-item>\n      <el-form-item label=\"运单号\">\n        <el-tooltip class=\"item\" effect=\"dark\" content=\"支持批量传入单号，单号与单号间用英文逗号分隔\" placement=\"top-start\">\n          <el-input v-model=\"form.waybill_list\" placeholder=\"运单号\" style=\"width: 200px\"></el-input>\n        </el-tooltip>\n      </el-form-item>\n      <el-form-item label=\"差错编码\">\n        <el-input :disabled=\"true\" v-model=\"lost_rep_code\" style=\"width: 200px\"></el-input>\n      </el-form-item>\n    </el-form>\n    <div style=\"padding: 0px 80px\">\n      <el-button type=\"primary\" @click=\"execute\">确认</el-button>\n    </div>\n  </div>\n\n</template>\n\n<script>\nimport Link from '@/model/dpf/link'\nimport Ubt from '@/model/dpf/ubt'\n\n\nexport default {\n  data() {\n    return {\n      form: {\n        waybill_type: 1,\n        waybill_list: '',\n      },\n      waybill_type_list: [{\n        label: '快递',\n        value: 1,\n      }, {\n        label: '零担',\n        value: 2,\n      }],\n      lost_rep_code: '',\n    }\n  },\n  methods: {\n    async execute() {\n      const res = await Link.doLoseGoods(this.form)\n      if (res.success) {\n        this.$message.success('处理成功')\n        this.lost_rep_code = res.result\n      } else {\n        this.$message.error(res.msg)\n      }\n\n      await Ubt.insertUbt('lose-goods')\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  //noinspection CssUnknownTarget\n  @import \"@/static/css/common.scss\";\n</style>\n"]}]}