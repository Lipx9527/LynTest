"form\" label-width=\"80px\" style=\"padding: 20px 0px\">\n      <el-form-item label=\"环境\">\n        <el-radio-group v-model=\"form.environment_id\">\n          <el-radio-button v-for=\"item in environment_id_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n      </el-form-item>\n\n      <el-form-item label=\"部门\">\n        <el-select v-model=\"form.department\"  style=\"width: 250px\">\n          <el-option v-for=\"item in department_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.value\"></el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-form-item label=\"运单号\">\n        <el-tooltip class=\"item\" effect=\"dark\" content=\"支持批量传入单号，单号与单号间用英文逗号分隔\" placement=\"top-start\">\n          <el-input v-model=\"form.order_numbers\" placeholder=\"运单号\" style=\"width: 200px\"></el-input>\n        </el-tooltip>\n      </el-form-item>\n\n      <el-form-item label=\"转退方式\">\n        <el-select v-model=\"form.return_mode\">\n          <el-option v-for=\"item in return_mode_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.value\"></el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-form-item label=\"转退原因\">\n        <el-select v-model=\"form.return_reason\" >\n          <el-option v-for=\"item in return_reason_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.value\"></el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-form-item label=\"转退类型\">\n        <el-select v-model=\"form.return_type\">\n          <el-option v-for=\"item in return_type_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.value\"></el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-form-item label=\"收货地址\">\n        <el-input v-model=\"form.return_province_name\" placeholder=\"省\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.return_city_name\" placeholder=\"市\" style=\"width: 80px\"></el-input>\n