learable placeholder=\"产品线\" style=\"width: 180px\" @change=\"getDevList\">\n          <el-option v-for=\"item in product_line_list\" :key=\"item.product_code\" :label=\"item.product_name\" :value=\"item.product_code\"/>\n        </el-select>\n      </el-row>\n    </el-card>\n    <br/>\n    <el-card>\n      <el-row>\n        <el-button type=\"primary\" size=\"small\" @click=\"insertDev\">新 增</el-button>\n        <el-pagination background\n                       @size-change=\"handleSizeChange\"\n                       @current-change=\"handleCurrentChange\"\n                       :current-page=\"search.page\"\n                       :page-size=\"search.count\"\n                       :page-sizes=\"[10, 20, 50, 100]\"\n                       :total=\"total\"\n                       layout=\"total, sizes, prev, pager, next\" style=\"float: right\"/>\n      </el-row>\n      <el-table :data=\"dev_list\">\n        <el-table-column prop=\"dev_name\" label=\"姓名\" sortable></el-table-column>\n        <el-table-column prop=\"dev_code\" label=\"工号\" sortable></el-table-column>\n        <el-table-column prop=\"product_name\" label=\"产品线\" sortable></el-table-column>\n        <el-table-column label=\"操作\" fixed=\"right\" width=\"150\">\n          <template slot-scope=\"scope\">\n            <el-button size=\"mini\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n            <el-button size=\"mini\" type=\"danger\" @click=\"deleteDev(scope.row.id)\">删除</el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n    </el-card>\n    <el-dialog :visible.sync=\"visible\" width=\"500px\">\n      <el-form :model=\"dev_info\" label-width=\"100px\">\n        <el-form-item label=\"姓名\">\n          <el-input placeholder=\"请输入开发姓名\" v-model=\"dev_info.dev_name\" style=\"width: 200px\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"工号\">\n          <el-input placeholder=\"请输入开发工号\" v-model=\"dev_info.dev_code\" style=\"width: 200px\"></el-input>