em v-for=\"second in rootModule.module\" :key=\"second.id\" :label=\"second.title\">\n            <el-switch v-model=\"second.switch_open\" active-color=\"#13ce66\" @change=\"modify(second)\"></el-switch>\n            <el-select v-model=\"second.code\"\n                       placeholder=\"请选择员工\"\n                       @change=\"modify(second)\"\n                       style=\"width: 100px;margin-left:20px\"\n                       v-show=\"second.switch_open\"\n                       size=\"small\">\n              <el-option v-for=\"item in courier_list\" :key=\"item.code\" :value =\"item.code\" :label =\"item.code\"></el-option>\n            </el-select>\n          </el-form-item>\n        </el-form>\n      </el-collapse-item>\n    </el-collapse>\n  </div>\n</template>\n\n<script>\nimport Uums from '@/model/dpf/uums'\n\nexport default {\n  name: 'ModuleConfig',\n  data() {\n    return {\n      module_info: [],\n      courier_list: [],\n    }\n  },\n  created() {\n    this.getAllModuleInfo()\n    this.getAllCourier()\n  },\n  methods: {\n    async getAllModuleInfo() {\n      this.module_info = await Uums.getModuleSwitchInfo()\n    },\n    async getAllCourier() {\n      this.courier_list = await Uums.getUumsInfoList()\n    },\n    async modify(val) {\n      const res = await Uums.modifyModuleSwitchInfo(val)\n      if (res.code < window.MAX_SUCCESS_CODE) {\n        this.$message.success(res.message)\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n//noinspection CssUnknownTarget\n@import \"@/static/css/common.scss\";\n</style>\n"]}]}