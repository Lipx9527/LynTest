>\n    <el-form ref=\"form\" :model=\"form\" label-width=\"80px\" style=\"padding: 20px 0px\">\n      <el-form-item label=\"环境\">\n        <el-radio-group v-model=\"form.environment_id\">\n          <el-radio-button v-for=\"item in environment_id_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n      </el-form-item>\n\n      <el-form-item label=\"运单号\">\n        <el-tooltip class=\"item\" effect=\"dark\" content=\"支持批量传入单号，单号与单号间用英文逗号分隔\" placement=\"top-start\">\n          <el-input v-model=\"form.order_numbers\" placeholder=\"运单号\" style=\"width: 200px\"></el-input>\n        </el-tooltip>\n      </el-form-item>\n\n      <el-form-item label=\"快递员\">\n        <el-select v-model=\"form.courier\">\n          <el-option v-for=\"item in courier_list\" :key=\"item\" :label =\"item\" :value=\"item\"></el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-form-item label=\"重量体积\">\n        <el-input v-model=\"form.weight\" placeholder=\"重量\" style=\"width: 80px\"></el-input>\n        <el-input v-model=\"form.volume\" placeholder=\"体积\" style=\"width: 80px\"></el-input>\n      </el-form-item>\n\n      <el-form-item label=\"超长货\">\n        <el-select v-model=\"form.alien_cargo_flag\">\n          <el-option v-for=\"item in alien_cargo_flag_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.value\"></el-option>\n        </el-select>\n      </el-form-item>\n    </el-form>\n    <el-button type=\"primary\" @click=\"dialog_visible = true\">拓展</el-button>\n    <el-button type=\"primary\" @click=\"execute\">激活</el-button>\n    <el-dialog :visible.sync=\"dialog_visible\">\n      <el-form ref=\"form\" :model=\"form\" label-width=\"80px\" size=\"small\">\n        <el-form-item label=\"其他费用\">\n          <el-input v-model=\"form.declared_value\" placeholder=\"保价费\" style=\"width: 80px\"></el-input>\n          <el-input v-model=\"form.package_fee\" placeholder=\"包装费\" style=\"width: 80px\"></el-input>\n          <el-input v-model=\"form.alien_cargo_fee\" placeholder=\"超长费\" style=\"width: 80px\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"海南液体\">\n          <el-input v-model=\"form.hm_liquid_fee\" placeholder=\"如有则填写\" style=\"width: 250px\"/>\n        </el-form-item>\n\n        <el-form-item label=\"拆木包装\">\n          <el-checkbox-group v-model=\"form.unpacking_mark\" :max=\"1\">\n            <el-checkbox-button v-for=\"item in unpacking_mark_list\" :label=\"item.value\" :key=\"item.value\">{{item.label}}</el-checkbox-button>\n          </el-checkbox-group>\n        </el-form-item>\n\n        <el-form-item label=\"拓展功能\">\n          <el-checkbox-group v-model=\"form.expend_func\">\n            <el-checkbox v-for=\"func in expend_func_list\" :label=\"func.value\" :key=\"func.value\">{{func.label}}</el-checkbox>\n          </el-checkbox-group>\n        </el-form-item>\n\n        <el-form-item label=\"超重\">\n          <el-input v-model=\"form.overweight_handling_service_fee\" placeholder=\"如有则填写\" style=\"width: 120px\"/>\n        </el-form-item>\n\n        <el-form-item label=\"超远\">\n          <el-input v-model=\"form.extra_fare_delivery_service_fee\" placeholder=\"如有则填写\" style=\"width: 120px\"/>\n        </el-form-item>\n\n        <el-form-item label=\"简易安装\">\n          <el-input v-model=\"form.easy_install_ation_fee\" placeholder=\"如有则填写\" style=\"width: 120px\"/>\n        </el-form-item>\n\n        <el-form-item label=\"通电验机\">\n          <el-input v-model=\"form.test_mac_hine_fee\" placeholder=\"如有则填写\" style=\"width: 120px\"/>\n        </el-form-item>\n\n        <el-form-item label=\"进仓服务\">\n          <el-switch v-model=\"form.is_warehousing_service\" active-color=\"#13ce66\"></el-switch>\n        </el-form-item>\n      </el-form>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\" @click=\"dialog_visible = false\">确 定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport Link from '@/model/dpf/link'\nimport Ubt from '@/model/dpf/ubt'\n\n\nexport default {\n  data() {\n    return {\n      form: {\n        environment_id: 1,\n        order_numbers: '',\n        courier: '196061',\n        weight: '5.0',\n        volume: '0.01',\n        alien_cargo_flag: 0,\n        declared_value: '',\n        package_fee: '',\n        alien_cargo_fee: '',\n        hm_liquid_fee: '',\n        unpacking_mark: [],\n        expend_func: [],\n        overweight_handling_service_fee: null,\n        extra_fare_delivery_service_fee: null,\n        easy_install_ation_fee: null,\n        test_mac_hine_fee: null,\n        is_warehousing_service: false,\n      },\n      dialog_visible: false,\n      environment_id_list: [{\n        label: '云1',\n        value: 1,\n      }, {\n        label: '云2',\n        value: 2,\n      }],\n      courier_list: ['196061', '175014', '171467', '158241', '196469', '193855', '310889'],\n      alien_cargo_flag_list: [{\n        label: '非超长货',\n        value: 0\n      }, {\n        label: '床垫1.5米非安装',\n        value: 1\n      }, {\n        label: '超长货',\n        value: 5\n      }],\n      unpacking_mark_list: [{\n        label: '是',\n        value: '1'\n      }, {\n        label: '非',\n        value: '2'\n      }],\n      expend_func_list: [{\n        label: '口令签收',\n        value: 'passwordSign'\n      }, {\n        label: '大连液体',\n        value: 'dlLiquid'\n      }, {\n        label: '非上机件',\n        value: 'breakableArticles'\n      }, {\n        label: '超长超重',\n        value: 'isLength'\n      }, {\n        label: '贵重包装',\n        value: 'isValuablePackag'\n      }]\n    }\n  },\n  methods: {\n    async execute() {\n      const res = await Link.doOrderActivate(this.form)\n      if (res.success) {\n        this.$message.success('激活成功')\n      } else {\n        this.$message.error(res.msg)\n      }\n\n      await Ubt.insertUbt('order-activate')\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n//noinspection CssUnknownTarget\n@import \"@/static/css/common.scss\";\n</style>\n"]}]}