select>\n      </el-form-item>\n    </el-form>\n    <div style=\"padding: 0px 80px\">\n      <el-button type=\"primary\" @click=\"execute\">补码</el-button>\n      <el-dialog title=\"补码结果\" :visible.sync=\"table_dialog_visible\">\n        <el-table :data=\"message\" style=\"width: 100%\" height=\"320\" stripe v-loading=\"loading\">\n          <el-table-column prop=\"waybillNo\" label=\"单号\" width=\"200\"></el-table-column>\n          <el-table-column prop=\"beSuccess\" label=\"状态\" width=\"60\"></el-table-column>\n          <el-table-column prop=\"message\" label=\"信息\"></el-table-column>\n        </el-table>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Link from '@/model/dpf/link'\nimport Ubt from '@/model/dpf/ubt'\n\nexport default {\n  data() {\n    return {\n      form: {\n        environment_id: 1,\n        order_number: '',\n        complement_type: 1,\n        department_value: 'W0113080102',\n      },\n      environment_id_list: [{\n        label: '云1',\n        value: 1,\n      }, {\n        label: '云2',\n        value: 2,\n      }],\n      complement_type_list: [{\n        label: '普通补码',\n        value: 1,\n      }, {\n        label: '零担补码',\n        value: 2,\n      }, {\n        label: '大件补码',\n        value: 3,\n      }],\n      department_list: this.GLOBAL.department,\n      loading: true,\n      table_dialog_visible: false,\n      message: [],\n    }\n  },\n  methods: {\n    async execute() {\n      this.loading = true\n      const res = await Link.doComplementCode(this.form)\n      if (res.success) {\n        this.table_dialog_visible = true\n        this.message = res.result\n        this.$message.success('补码成功')\n        this.loading = false\n      } else {\n        this.$message.error(res.msg)\n      }\n\n      await Ubt.insertUbt('complement-code')\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n//noinspection CssUnknownTarget\n@import \"@/static/css/common.scss\";\n</style>\n"]}]}