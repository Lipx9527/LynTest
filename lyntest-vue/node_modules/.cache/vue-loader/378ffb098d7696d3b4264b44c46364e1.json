>\n\n      <el-form-item label=\"批量开单\">\n        <el-input-number v-model=\"form.batch_orders\"  @change=\"handleBatchOrders\" :min=\"1\" :max=\"20\" label=\"批量开单\"></el-input-number>\n      </el-form-item>\n\n    </el-form>\n\n    <div style=\"padding: 0px 12px\">\n      <el-button type=\"primary\" @click=\"getOrderId\">立即创建</el-button>\n      <el-dialog title=\"订单号\" :visible.sync=\"order_list_dialog_Visible\">\n        <el-input type=\"textarea\" autosize  v-model=\"order_list\" v-loading=\"loading\"></el-input>\n        <span slot=\"footer\" class=\"dialog-footer\">\n              <el-button type=\"primary\" @click=\"order_list_dialog_Visible = false\" >确 定</el-button>\n            </span>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Orders from '@/model/dpf/orders'\nimport Ubt from '@/model/dpf/ubt'\n\nexport default {\n  data() {\n    return {\n      form: {\n        channel: 1,\n        batch_orders: 1\n      },\n      channel_list: [{\n        value: 1,\n        label: '内部发货'\n      }, {\n        value: 2,\n        label: '内部带货'\n      }],\n      order_list_dialog_Visible: false,\n      loading: true,\n      order_list: ''\n\n    }\n  },\n  methods: {\n    async getOrderId() {\n      this.order_list = ''\n      this.order_list_dialog_Visible = true\n      this.loading = true\n      this.order_list = await Orders.getInternalOrder(this.form)\n      this.loading = false\n\n      await Ubt.insertUbt('internal-order', this.form.batch_orders)\n    }\n  }\n}\n</script>\n\n<!--suppress CssUnknownTarget -->\n<style lang=\"scss\" scoped>\n  @import \"@/static/css/common.scss\";\n</style>\n"]}]}