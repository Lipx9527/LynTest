lor=\"#13ce66\"\n                   active-text=\"云2\"\n                   inactive-text=\"云1\"\n                   :active-value=\"2\"\n                   :inactive-value=\"1\"/>\n      </el-form-item>\n      <el-form-item label=\"类型\">\n        <el-select v-model=\"form.type\" :value=\"form.type\" style=\"width: 250px\" @change=\"handleType\">\n          <el-option v-for=\"item in type_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.value\"></el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"运单号\">\n        <el-tooltip class=\"item\" effect=\"dark\" content=\"支持批量传入单号，单号与单号间用英文逗号分隔\" placement=\"top-start\">\n          <el-input v-model=\"form.waybill_no\" style=\"width: 250px\"/>\n        </el-tooltip>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"submit\">提交</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n\n<script>\nimport Pda from '@/model/dpf/pda'\n\nexport default {\n  data() {\n    return {\n      form: {\n        environment_id: 2,\n        type: 1,\n        waybill_no: '',\n      },\n      switch_disabled: true,\n      type_list: [{\n        label: '电子签名图片',\n        value: 1\n      }, {\n        label: '底单',\n        value: 2\n      }, {\n        label: '拆包装前',\n        value: 3\n      }, {\n        label: '拆包装后',\n        value: 4\n      }]\n    }\n  },\n  methods: {\n    handleType(type) {\n      if (type > 1) {\n        this.switch_disabled = false\n        return\n      }\n      if (type === 1) {\n        this.form.environment_id = 2\n        this.switch_disabled = true\n      }\n    },\n    async submit() {\n      const res = await Pda.imageUpload(this.form)\n      if (res.success) {\n        this.$message.success(res.result)\n      } else {\n        this.$message.error(res.msg)\n      }\n    }\n  }\n}\n</script>\n\n\n<style lang=\"scss\" scoped>\n//noinspection CssUnknownTarget\n@import \"@/static/css/common.scss\";\n</style>\n"]}]}