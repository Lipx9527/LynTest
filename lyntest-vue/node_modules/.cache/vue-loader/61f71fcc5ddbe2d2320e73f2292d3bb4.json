             </el-radio-group>\n            </el-form-item>\n\n            <el-form-item label=\"总件数\">\n              <el-radio-group v-model=\"form.total_count\">\n                <el-radio-button v-for=\"item in total_count_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n              </el-radio-group>\n            </el-form-item>\n\n            <el-form-item label=\"代收货款\">\n              <el-radio-group v-model=\"form.bool_collecting\" @change=\"searchCaseList\">\n                <el-radio-button v-for=\"item in bool_collecting_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n              </el-radio-group>\n            </el-form-item>\n\n            <el-form-item label=\"走货路径\">\n              <el-select v-model=\"form.transport_path\" @change=\"searchCaseList\">\n                <el-option v-for=\"item in transport_path_list\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\"></el-option>\n              </el-select>\n            </el-form-item>\n\n            <el-form-item label=\"返单类型\">\n              <el-select v-model=\"form.back_order\" @change=\"searchCaseList\">\n                <el-option v-for=\"item in back_order_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.value\"></el-option>\n              </el-select>\n              <el-select v-model=\"form.return_requirement\" multiple placeholder=\"返单要求\" style=\"width: 160px\">\n                <el-option v-for=\"item in return_requirement_list\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\"></el-option>\n              </el-select>\n            </el-form-item>\n\n            <el-form-item label=\"手机号\">\n              <el-input v-model=\"form.delivery_mobile\" placeholder=\"发货人手机号\" style=\"width: 180px\"></el-input>\n              <el-input v-model=\"form.receiver_mobile\" placeholder=\"收货人手机号\" style=\"width: 180px\"></el-input>\n            </el-form-item>\n          </el-form>\n        </vue-scroll>\n      </div>\n    </el-drawer>\n\n    <el-dialog title=\"请求入参\" :close-on-click-modal=false :visible.sync=\"params_dialog_visible\">\n      <el-input type=\"textarea\" :rows=\"3\" placeholder=\"描述\"  v-model=\"form.description\"></el-input>\n      <el-input type=\"textarea\" :rows=\"10\" placeholder=\"请输入请求入参\"  v-model=\"form.form_data\" style=\"margin-top: 10px\"></el-input>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"params_dialog_visible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"saveFormData\">确 定</el-button>\n      </span>\n    </el-dialog>\n\n    <el-dialog title=\"运单号\" :close-on-click-modal=false :visible.sync=\"result_dialog_visible\">\n      <el-input type=\"textarea\" autosize  v-model=\"order_list\" v-loading=\"loading\"></el-input>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\" @click=\"result_dialog_visible = false\" >确 定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport WayBill from '@/model/dpf/waybill'\nimport Ubt from '@/model/dpf/ubt'\n\nexport default {\n  data() {\n    return {\n      user_code: null,\n      operate_type: null,\n      case_list: [],\n      loading: false,\n      params_dialog_visible: false,\n      result_dialog_visible: false,\n      order_list: '', // 结果\n      dra_show: false,\n      form: {},\n      default_form: {\n        type: 1,\n        look_self: false,\n        environment_id: 1,\n        waybill_begin_type: 1,\n        goods_weight: 1,\n        transport_type: '精准卡航',\n        value_added_services: 1,\n        pick_type: 1,\n        payment_type: 1,\n        total_count: 1,\n        bool_collecting: 1,\n        transport_path: 1, // 走货路径\n        back_order: 1,\n        return_requirement: [],\n        delivery_mobile: '',\n        receiver_mobile: '',\n        batch_orders: 1,\n        description: '',\n        form_data: '',\n      },\n      type_list: [{\n        label: '直营',\n        value: 1\n      }, {\n        label: '合伙人',\n        value: 2\n      }],\n      environment_id_list: [{\n        label: '私有云1',\n        value: 1\n      }, {\n        label: '私有云2',\n        value: 2\n      }],\n      waybill_begin_type_list: [{\n        label: '9位数字',\n        value: 1\n      }, {\n        label: 'DPL开头15位',\n        value: 2\n      }],\n      goods_weight_list: [{\n        label: '重货',\n        value: 1\n      }, {\n        label: '轻货',\n        value: 2\n      }],\n      transport_type_list: ['精准卡航', '小包', '精准包裹', '精准汽运(短途)', '汽运偏线', '精准汽运(长途)', '精准城运', '精准空运'],\n      value_added_services_list: [{\n        label: '无',\n        value: 1\n      }, {\n        label: '家具送装',\n        value: 2\n      }, {\n        label: '建材送装',\n        value: 3\n      }, {\n        label: '家电送装',\n        value: 4\n      }, {\n        label: '卫浴送装',\n        value: 5\n      }],\n      pick_type_list: [{\n        label: '自提',\n        value: 1\n      }, {\n        label: '送货进仓',\n        value: 2\n      }, {\n        label: '送货上楼',\n        value: 3\n      }, {\n        label: '送货(不含上楼)',\n        value: 4\n      }, {\n        label: '大件上楼',\n        value: 5\n      }, {\n        label: '内部带货送货',\n        value: 11\n      }, {\n        label: '内部带货自提',\n        value: 12\n      }],\n      pick_type_one: [{\n        label: '自提',\n        value: 1\n      }, {\n        label: '送货进仓',\n        value: 2\n      }, {\n        label: '送货上楼',\n        value: 3\n      }, {\n        label: '送货(不含上楼)',\n        value: 4\n      }, {\n        label: '大件上楼',\n        value: 5\n      }, {\n        label: '内部带货送货',\n        value: 11\n      }, {\n        label: '内部带货自提',\n        value: 12\n      }],\n      pick_type_two: [{\n        label: '送货不上楼(家装)',\n        value: 6\n      }, {\n        label: '送货上楼(家装)',\n        value: 7\n      }, {\n        label: '送货安装',\n        value: 8\n      }, {\n        label: '送货入户',\n        value: 9\n      }, {\n        label: '大件上楼(家装)',\n        value: 10\n      }],\n      payment_type_list: [{\n        value: 1,\n        label: '现金'\n      }, {\n        value: 2,\n        label: '到付',\n      }, {\n        label: '月结',\n        value: 3\n      }, {\n        label: '临时欠款',\n        value: 4\n      }],\n      total_count_list: [{\n        label: '单件',\n        value: 1\n      }, {\n        label: '多件',\n        value: 2\n      }],\n      bool_collecting_list: [{\n        label: '无',\n        value: 1\n      }, {\n        label: '有',\n        value: 2\n      }],\n      transport_path_list: [{\n        label: '上海-上海营业部',\n        value: 1\n      }, {\n        label: '上海-北京',\n        value: 2\n      }, {\n        label: '上海-合伙人',\n        value: 3\n      }, {\n        label: '合伙人-上海',\n        value: 4\n      }, {\n        label: '合伙人-北京',\n        value: 5\n      }, {\n        label: '上海-上海派送部',\n        value: 6\n      }],\n      back_order_list: [{\n        label: '无返单',\n        value: 1\n      }, {\n        label: '客户签收单传真返回',\n        value: 2\n      }, {\n        label: '客户签收单原件返回',\n        value: 3\n      }, {\n        label: '运单电子版返回',\n        value: 4\n      }],\n      return_requirement_list: [{\n        value: 'R1',\n        label: '签名'\n      }, {\n        value: 'R2',\n        label: '盖章'\n      }, {\n        value: 'R3',\n        label: '签身份证号'\n      }, {\n        value: 'R4',\n        label: '身份证复印件'\n      }, {\n        value: 'R5',\n        label: '仓库收货回执单'\n      }],\n    }\n  },\n  mounted() {\n    this.form = { ...this.default_form }\n    this.getUserInfo()\n    this.searchCaseList()\n  },\n  methods: {\n    async searchCaseList() {\n      this.case_list = await WayBill.getLtlCaseList(this.form)\n    },\n    getUserInfo() {\n      const userInfo = JSON.parse(window.localStorage.getItem('vuex'))\n      this.user_code = userInfo.user.code\n    },\n    createNewCase() {\n      this.operate_type = 1\n      this.params_dialog_visible = true\n    },\n    editFormData(row) {\n      this.operate_type = 2\n      this.params_dialog_visible = true\n      this.form.id = row.id\n      this.form.description = row.description\n      this.form.form_data = row.form_data\n    },\n    handleValueAddedServices(value) {\n      // 增值服务\n      this.form.value_added_services = value\n      if (value === 1) {\n        this.form.pick_type = 1\n        this.pick_type_list = this.pick_type_one\n      } else if (value > 1) {\n        this.form.pick_type = 6\n        this.pick_type_list = this.pick_type_two\n      }\n    },\n    async saveFormData() {\n      const res = this.operate_type === 1 ? await WayBill.createLtlCaseInfo(this.form) : await WayBill.updateLtlCaseInfo(this.form)\n      this.params_dialog_visible = false\n      this.searchCaseList()\n      this.$message.success(res)\n    },\n    async getOrder(row) {\n      this.result_dialog_visible = true\n      this.loading = true\n      Object.keys(this.form).forEach(i => {\n        if (!row.hasOwnProperty(i)) {\n          row[i] = this.form[i]\n        }\n      })\n      row.batch_orders = this.form.batch_orders\n      this.order_list = await WayBill.getLtlOrder(this.form)\n      this.loading = false\n\n      await Ubt.insertUbt('ltl-waybill', this.form.batch_orders)\n    },\n    async deleteCase(id) {\n      const res = await WayBill.deleteLtlCase(id)\n      this.searchCaseList()\n      this.$message.success(res)\n    },\n    getLabelFromValue(l, value) {\n      for (const item of l) {\n        if (item.value === value) {\n          return item.label\n        }\n      }\n    }\n  }\n}\n</script>\n\n\n<!--suppress CssUnknownTarget -->\n<style lang=\"scss\" scoped>\n@import \"@/static/css/common.scss\";\n.header-form-list {\n  display: flex;\n  flex-wrap: wrap;\n\n  .header-form {\n    display: flex;\n    align-items: center;\n    margin-right: 30px;\n    margin-bottom: 10px;\n\n    .title {\n      font-size: medium;\n      font-weight: bold;\n      margin-right: 16px;\n      white-space: nowrap;\n    }\n  }\n\n\n}\n</style>\n"]}]}