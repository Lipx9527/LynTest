;;;;;;;;;;;;;;;;;;;;;;;AAiQA","file":"mid-data-platform.vue","sourceRoot":"src/view/data-platform/order","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"header\"><div class=\"title\">电子运单下单</div></div>\n    <el-form ref=\"form\" :model=\"form\" label-width=\"80px\" style=\"padding: 20px 0px\">\n      <el-form-item label=\"环境\">\n        <el-radio-group v-model=\"form.environment_id\">\n          <el-radio-button v-for=\"item in environment_id_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n      </el-form-item>\n      <el-form-item label=\"订单来源\">\n        <el-radio-group v-model=\"form.source_of_order\">\n          <el-radio-button v-for=\"item in source_of_orders_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n      </el-form-item>\n      <el-form-item label=\"订单类型\">\n        <el-select v-model=\"form.order_type\">\n          <el-option v-for=\"item in order_type_list\" :key=\"item.value\" :label =\"item.label\" :value=\"item.note\">\n            <span style=\"float:left\">{{item.label}}</span>\n            <span style=\"float: right; color: #8492a6; font-size: 13px\">{{item.note}}</span>\n          </el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"子母件\" >\n        <el-radio-group v-model=\"form.goods_count\" @change=\"handleGoodsCount\">\n          <el-radio-button v-for=\"item in goods_count_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n      </el-form-item>\n      <el-form-item label=\"子母件数\">\n        <el-input-number v-model=\"form.batch_goods_count\"  @change=\"handleBatchGoodsCount\" :min=\"1\" :max=\"99\"></el-input-number>\n      </el-form-item>\n      <el-form-item label=\"大客户\">\n        <el-radio-group v-model=\"form.waybill_vip\" >\n          <el-radio-button v-for=\"item in waybill_vip_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n      </el-form-item>\n      <el-form-item label=\"返单类型\">\n        <el-radio-group v-model=\"form.return_bill_type\" >\n          <el-radio-button v-for=\"item in return_bill_type_list\" :key=\"item.value\" :label=\"item.value\">{{item.label}}</el-radio-button>\n        </el-radio-group>\n      </el-form-item>\n      <el-form-item label=\"客户编码\">\n        <el-input placeholder=\"请输入客户编码\" v-model=\"form.shipper_id\" style=\"width: 180px\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"批量开单\">\n        <el-input-number v-model=\"form.batch_orders\" :min=\"1\" :max=\"30\" :disabled=\"batch_orders_disabled\"></el-input-number>\n      </el-form-item>\n    </el-form>\n    <div style=\"padding: 0px 12px\">\n      <el-button type=\"primary\" @click=\"insertOrders\">立即创建</el-button>\n      <el-dialog title=\"订单号(默认为订单号，去除首字母O/T，则为运单号)\" :visible.sync=\"order_list_dialog_Visible\">\n        <el-input type=\"textarea\" autosize  v-model=\"order_list\" v-loading=\"loading\"></el-input>\n        <span slot=\"footer\" class=\"dialog-footer\">\n          <el-button type=\"primary\" @click=\"order_list_dialog_Visible = false\" >确 定</el-button>\n        </span>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Orders from '@/model/dpf/orders'\nimport Ubt from '@/model/dpf/ubt'\n\nexport default {\n  data() {\n    return {\n      loading: true,\n      order_list_dialog_Visible: false,\n      order_list: '',\n      form: {\n        environment_id: 1,\n        source_of_order: 'NVAS',\n        order_type: 'PACKAGE',\n        goods_count: 1, // 子母件，默认为单件\n        batch_goods_count: 1, // 子母件数\n        waybill_vip: 2, // 电子运单大客户，默认为是\n        return_bill_type: 1,\n        batch_orders: 1, // 件数\n        shipper_id: '401255708', // 客户编码\n      },\n      batch_orders_disabled: false,\n      environment_id_list: [{\n        label: '云1',\n        value: 1,\n      }, {\n        label: '云2',\n        value: 2,\n      }],\n      source_of_orders_list: [{\n        label: 'Nvas',\n        value: 'NVAS'\n      }, {\n        label: '淘宝',\n        value: 'TAOBAO'\n      }, {\n        label: '京东',\n        value: 'JINGDONG'\n      }],\n      // 2020年09月24日 艳云要求将标准快递设为默认值\n      order_type_list: [{\n        label: '标准快递',\n        note: 'PACKAGE',\n        value: 1\n      }, {\n        label: '精准汽运长途',\n        note: 'JZQY_LONG',\n        value: 2\n      }, {\n        label: '电商尊享',\n        note: 'EPEP',\n        value: 3\n      }, {\n        label: '3.60特惠件',\n        note: 'RCP',\n        value: 4\n      }, {\n        label: '精准空运',\n        note: 'JZKY',\n        value: 5\n      }, {\n        label: '精准汽运短途',\n        note: 'JZQY_SHORT',\n        value: 6\n      }, {\n        label: '特准快递',\n        note: 'DEAP',\n        value: 7\n      }, {\n        label: '精准卡航',\n        note: 'JZKH',\n        value: 8\n      }, {\n        label: '精准诚运',\n        note: 'JZCY',\n        value: 9\n      }, {\n        label: '汽运偏线',\n        note: 'AGENT_VEHICLE',\n        value: 10\n      }, {\n        label: '精准大票-标准件',\n        note: 'YTY',\n        value: 11\n      }, {\n        label: '精准大票-经济件',\n        note: 'DTD',\n        value: 12\n      }, {\n        label: '快递报关通-标',\n        note: 'GTSE',\n        value: 13\n      }, {\n        label: '国际快递-标',\n        note: 'ICES',\n        value: 14\n      }, {\n        label: '国际快递-快',\n        note: 'ICEC',\n        value: 15\n      }, {\n        label: '快递报关通-快',\n        note: 'GTEC',\n        value: 16\n      }, {\n        label: '精准包裹',\n        note: 'PCP',\n        value: 17\n      }, {\n        label: '3.60特重件',\n        note: 'ZBRH',\n        value: 18\n      }, {\n        label: '11.27',\n        note: '测试11.27',\n        value: 19\n      }, {\n        label: '重包入户',\n        note: 'NZBRH',\n        value: 20\n      }, {\n        label: '重包特惠',\n        note: 'ZBTH',\n        value: 21\n      }, {\n        label: '经济大件',\n        note: 'JJDJ',\n        value: 22\n      }, {\n        label: '微小件特惠',\n        note: 'WXJTH',\n        value: 23\n      }],\n      goods_count_list: [{\n        label: '否',\n        value: 1\n      }, {\n        label: '是',\n        value: 2\n      }],\n      waybill_vip_list: [{\n        label: '否',\n        value: 1\n      }, {\n        label: '是',\n        value: 2\n      }],\n      return_bill_type_list: [{\n        label: '无需返单',\n        value: 1\n      }, {\n        label: '客户签收单原件返回',\n        value: 2\n      }, {\n        label: '电子签收单返回',\n        value: 3\n      }],\n    }\n  },\n  methods: {\n    handleGoodsCount(value) {\n      // 子母件\n      this.form.default_goods_count = value\n      this.form.batch_goods_count = value\n      if (value === 2) {\n        this.form.batch_orders_disabled = true\n        this.form.batch_orders = 1\n      } else if (value === 1) {\n        this.form.batch_orders_disabled = false\n      }\n    },\n    handleBatchGoodsCount(value) {\n      // 子母件数\n      this.form.batch_goods_count = value\n      if (value >= 2) {\n        this.form.default_goods_count = 2\n        this.form.batch_orders_disabled = true\n        this.form.batch_orders = 1\n      } else if (value === 1) {\n        this.form.default_goods_count = 1\n        this.form.batch_orders_disabled = false\n      }\n    },\n    async insertOrders() {\n      this.order_list = ''\n      this.order_list_dialog_Visible = true\n      this.loading = true\n      this.order_list = await Orders.getMidOrder(this.form)\n      this.loading = false\n\n      await Ubt.insertUbt('electronic-waybill-order', this.form.batch_orders)\n    }\n  }\n}\n</script>\n\n<!--suppress CssUnknownTarget -->\n<style lang=\"scss\" scoped>\n  @import \"@/static/css/common.scss\";\n</style>\n"]}]}